{
  "name": "in-need-of-time",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "docker compose up -d && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "ai": "npx tsx ./agent/index.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down && docker volume rm $(docker volume ls -q --filter name=neon_data) && docker-compose up -d",
    "db:push": "drizzle-kit push",
    "db:studio": "NODE_ENV=development drizzle-kit studio",
    "db:migrate": "drizzle-kit generate && drizzle-kit push"
  },
  "simple-git-hooks": {
    "pre-commit": "npx pretty-quick --staged"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
